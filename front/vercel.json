{
  "version": 2,
  "routes": [
  
    {
      "src": "/(.*\\.(ico|css|js|gif|jpg|jpeg|png|svg|woff2))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    
    {
      "src": "/(.*)",
      "dest": "/front/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}