{
    "version": 2,
    "rewrites": [
      {
        "src": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "src": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          }
        ]
      },
      {
        "src": "/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }